# Ignore database.yml file
/config/database.yml

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/db/backups
public/uploads
.idea
*.DS_Store
coverage
.*swp
/uploads/*

# Ignore node packages
/node_modules

test/reports
/public/packs
/public/packs-test
/node_modules
yarn-debug.log*
.yarn-integrity
