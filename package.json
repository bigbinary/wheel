    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "core-js": "3.7.0",
    "formik": "^2.2.5",
    "js-logger": "^1.6.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-router-nav-prompt": "^0.4.0",
    "react-toastify": "^6.1.0",
    "react_ujs": "^2.6.1",
    "yup": "^0.31.0"
